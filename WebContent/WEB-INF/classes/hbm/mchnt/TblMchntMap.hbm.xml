<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po.mchnt">
	<class
		name="TblMchntMap"
		table="TBL_MCHNT_MAP"
	>
<!--		<meta attribute="sync-DAO">true</meta>-->
		<composite-id name="id" class="TblMchntMapPK">
			<key-property
				name="mappingId"
				column="MAPPING_ID"
				type="com.huateng.hibernate.dataType.TrimString"
			/>
		</composite-id>
		
		<property
			name="mchntId"
			column="MCHNT_ID"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="equipmentId"
			column="EQUIPMENT_ID"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="acquiresId"
			column="ACQUIRERS_ID"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="createPerson"
			column="CREATE_PERSON"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="remark"
			column="remark"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		
		<property
			name="acmchntId"
			column="ACMCHNT_ID"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
		<property
			name="acequipmentId"
			column="ACEQUIPMENT_ID"
			type="com.huateng.hibernate.dataType.TrimString"
		/>
	</class>	
</hibernate-mapping>