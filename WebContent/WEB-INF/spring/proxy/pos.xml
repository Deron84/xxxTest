    <bean id="tblTermManagementDAO" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="proxyInterfaces">
            <value>com.huateng.dao.iface.term.TblTermManagementDAO</value>
        </property>
        <property name="interceptorNames">
            <value>tblTermManagementDAOTarget</value>
        </property>
    </bean>
    <bean id="tblTermManagementDAOTarget" class="com.huateng.dao.impl.term.TblTermManagementDAO">
        <property name="sessionFactory">
            <ref bean="sessionFactory" />
        </property>
    </bean>
    <bean id="t3030BO" class="com.huateng.bo.impl.term.TermManagementBOImpl">
        <property name="tblTermManagementDAO">
            <ref bean="tblTermManagementDAO"/>
        </property>
         <property name="tblTermInfDAO">
            <ref bean="TblTermInfDAO"/>
        </property> 
        <property name="tblTermInfTmpDAO">
            <ref bean="TblTermInfTmpDAO" />
        </property>
        <property name="commQueryDAO">
            <ref bean="CommQueryDAO"/>
        </property>       
    </bean>
    <bean id="t30301Action" class="com.huateng.struts.pos.action.T30301Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
        <property name="methods">
            <map>
                <!-- 入库 -->
                <entry key="add" value="storeTerminal"/>
                <!-- 批量入库 -->
                <entry key="adds" value="storeTerminals"/>
                <!-- 作废-->
                <entry key="invalid" value="invalidTerminal"/>
            </map>
          </property>
    </bean>
    <bean id="t30302Action" class="com.huateng.struts.pos.action.T30302Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
    </bean>
    <bean id="t30303Action" class="com.huateng.struts.pos.action.T30303Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
        <property name="methods">
            <map>
                <!-- 通过 -->
                <entry key="00" value="reciTerminal"/>
                <!-- 批次通过-->
                <entry key="01" value="reciTerminals"/>
                <!-- 拒绝 -->
                <entry key="10" value="refuseTerminal"/>
                <!-- 批次拒绝 -->
                <entry key="11" value="refuseTerminals"/>
           </map>
        </property>
    </bean>
    <bean id="t30304Action" class="com.huateng.struts.pos.action.T30304Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
    </bean>
    <bean id="t30305Action" class="com.huateng.struts.pos.action.T30305Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
        <property name="t3010BO">
            <ref bean="t3010BO"/>
        </property>
    </bean>
    <bean id="t30306Action" class="com.huateng.struts.pos.action.T30306Action" singleton="false">
        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
        <property name="methods">
            <map>
                <!-- 通过 -->
                <entry key="0" value="signTerminal"/>
                <!-- 批次通过-->
                <entry key="1" value="signTerminals"/>
           </map>
        </property>
    </bean>
    <!-- end 终端库存-->
    <bean id="termService" class="org.springframework.aop.framework.ProxyFactoryBean">
		<property name="proxyInterfaces">
			<value>com.huateng.bo.term.TermService</value>
		</property>
		<property name="interceptorNames">
			<list>
				<value>transactionInterceptor</value>
				<value>termServiceImpl</value>
			</list>
		</property>
	</bean>
    <bean id="termServiceImpl" class="com.huateng.bo.impl.term.TermServiceImpl">
    	<property name="dao" ref="hqlDao"/>
    	<property name="sqlDao" ref="sqlDao"/>
    </bean>
