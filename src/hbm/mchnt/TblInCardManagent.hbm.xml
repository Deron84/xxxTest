<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po.mchnt">
	<class
		name="TblInCardManagent"
		table="tbl_in_card_managent"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="TblInCardManagentPK">
			<key-property
				name="mchntCd"
				column="mcht_No"
				type="string"
			/>
			<key-property
				name="inCardNum"
				column="IN_CARDNUM"
				type="string"
			/><key-property
				name="inOutFlag"
				column="in_out_flag"
				type="string"
			/>
		</composite-id>
		<property
			name="termId"
			column="TERM_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="contactTel"
			column="CONTACT_TEL"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="contactName"
			column="CONTACT_NAME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="contactIdentify"
			column="CONTACT_IDENTIFY"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="inFlag"
			column="IN_FLAG"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="useFlag"
			column="USE_FLAG"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="messageLimit"
			column="MESSAGE_LIMIT"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="dealBeginTime"
			column="DEAL_BEGINTIME"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="dealEndTime"
			column="DEAL_ENDTIME"
			type="string"
			not-null="false"
			length="20"
		/>
	</class>	
</hibernate-mapping>